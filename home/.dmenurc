#!/bin/bash
# makes dmenu identical across all scripts by sourcing this file
#
# multi-selection patch: http://lists.suckless.org/dev/0109/26302.html

__font='Terminus-9'

__nb=#1d1f21    # normal bg
__nf=#7f8185    # normal fg
__sb=#1d1f21    # selected bg
__sf=#c5c8c6    # selected fg

DMENU="dmenu -i -fn $__font -nb $__nb -nf $__nf -sb $__sb -sf $__sf "
