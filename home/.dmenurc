#!/bin/bash
# makes dmenu identical across all scripts by sourcing this file
#
# !NOTE! needs to define global DMENU
#

readonly __font='Terminus-9'

readonly __nb=#1d1f21    # normal bg
readonly __nf=#7f8185    # normal fg
readonly __sb=#1d1f21    # selected bg
readonly __sf=#c5c8c6    # selected fg

readonly DMENU="dmenu -i -fn $__font -nb $__nb -nf $__nf -sb $__sb -sf $__sf "
